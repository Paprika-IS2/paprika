<h1><%= t('hello') %></h1>

<% flash.each do |key, value| %>
    <div class="alert alert-<%= key %> alert-dismissible fade-in"  id="alert-app" role="alert">
      <button type="button" class="close" data-dismiss="alert" aria-label="Close" >
        <span aria-hidden="true">&times;</span>
      </button>
      <%= value %>
    </div>
<% end %>
